{
	"expo": {
		"plugins": [
			[
				"expo-camera",
				{
					"cameraPermission": "Allow $(test) to access your camera"
				}
			],
			[
				"expo-media-library",
				{
					"photosPermission": "Allow $(test) to access your photos.",
					"savePhotosPermission": "Allow $(test) to save photos.",
					"isAccessMediaLocationEnabled": true
				}
			],
			[
				"expo-location",
				{
					"locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
				}
			]
		],
		"name": "picture-map-app-2",
		"slug": "picture-map-app-2",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"favicon": "./assets/images/favicon.png"
		}
	}
}
